  "homepage": "https://github.com/yhay81/GASlacker#readme",
  "devDependencies": {
    "@types/google-apps-script": "1.0.10",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "cpx": "^1.5.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
